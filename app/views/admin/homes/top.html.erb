<h2>注文履歴一覧</h2>

<table>
  <th>注文日</th>
  <th>購入者</th>
  <th>注文個数</th>
  <th>注文ステータス</th>


  <% @orders.each do |orders| %>
  <tr>

    <td>
      <%= link_to admin_order_path(orders.id) do %>
        <%= orders.created_at.strftime('%Y/%m/%d %H:%M') %>
      <% end %>
    </td>

    <td><%= orders.customer.first_name %><%= orders.customer.last_name %></td>

    <td>
      <% orders.order_details.each do |order_detail| %>
        <%= order_detail.amount %>
      <% end %>
    </td>

    <td><%= orders.status %></td>







  </tr>
 <% end %>
</table>
